{
  "name": "react-light-player",
  "version": "0.0.6",
  "description": "An Light Web Player",
  "author": "",
  "license": "MIT",
  "keywords": [
    "react",
    "react-component",
    "components"
  ],
  "main": "dist/ReactLightPlayer.js",
  "module": "src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "test": "jest --colors --coverage",
    "build-dev": "rm -rf build && webpack --config webpack.dev.js --env.NODE_ENV=local --env.production --progress",
    "build-prod": "rm -rf build && cross-env NODE_ENV=production webpack --config webpack.prod.js --progress",
    "build-npm": "rm -rf dist && npm run upgrade-version && cross-env NODE_ENV=npm webpack --config webpack.npm.js --progress",
    "build-npm-test": "rm -rf dist && cross-env NODE_ENV=npm webpack --config webpack.npm.test.js --progress",
    "transpile": "npm run build-npm",
    "prepublishOnly": "npm run transpile",
    "upgrade-version": "gulp",
    "deploy": "gh-pages -d build/prod",
    "publish-demo": "npm run build-prod && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NikFranki/react-light-player"
  },
  "bugs": {
    "url": "https://github.com/NikFranki/react-light-player/issues"
  },
  "dependencies": {
    "antd": "^3.9.2",
    "js-cookie": "^2.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^7.1.2",
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.3.1",
    "cross-env": "^5.0.5",
    "events": "^3.0.0",
    "file-loader": "^1.1.11",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.0",
    "gulp-bump": "^3.1.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "react-light-player": "^0.0.1",
    "react-load-image": "^0.1.7",
    "react-test-renderer": "^16.8.1",
    "style-loader": "^0.18.2",
    "swiper": "^4.4.6",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.4"
  }
}
